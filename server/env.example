# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration
DB_PATH=./space_traffic.db

# API Keys for Satellite Tracking Services
# Space-Track.org (requires registration at https://www.space-track.org/)
SPACE_TRACK_USERNAME=your_username
SPACE_TRACK_PASSWORD=your_password

# N2YO API (free tier available at https://www.n2yo.com/api/)
N2YO_API_KEY=your_api_key

# Satellite Tracker API (alternative service)
SATELLITE_TRACKER_API_KEY=your_api_key

# Update Intervals (in seconds)
SATELLITE_UPDATE_INTERVAL=30
COLLISION_CHECK_INTERVAL=60

# Mock Data Configuration
USE_MOCK_DATA=true
MOCK_SATELLITE_COUNT=10
